{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"env": {
				"GITHUB_TOKEN": "${config:p7djs.gh_token}",
				"GITHUB_HOOK_SECRET": "${config:p7djs.ghwh_secret}"
			},
			"program": "${workspaceFolder}\\example\\bot.js"
		}
	]
}
